            <div class="wraper container-fluid">
                <div class="page-title"> 
                    <h3 class="title">Form Wizard</h3> 
                </div>

                <!-- Basic Form Wizard -->
                <div class="row">
                    <div class="col-md-12">

                        <div class="panel panel-default">
                            <div class="panel-heading"> 
                                <h3 class="panel-title">Basic Form Wizard</h3> 
                            </div> 
                            <div class="panel-body"> 
                                <form id="basic-form" action="#">
                                    <div id="steps-uid-0" class="wizard clearfix" role="application"><div class="steps clearfix"><ul role="tablist"><li aria-selected="true" aria-disabled="false" class="first current" role="tab"><a id="steps-uid-0-t-0" href="#steps-uid-0-h-0" aria-controls="steps-uid-0-p-0"><span class="current-info audible">current step: </span><span class="number">1.</span> Account</a></li><li aria-disabled="true" class="disabled" role="tab"><a id="steps-uid-0-t-1" href="#steps-uid-0-h-1" aria-controls="steps-uid-0-p-1"><span class="number">2.</span> Profile</a></li><li aria-disabled="true" class="disabled" role="tab"><a id="steps-uid-0-t-2" href="#steps-uid-0-h-2" aria-controls="steps-uid-0-p-2"><span class="number">3.</span> Hints</a></li><li aria-disabled="true" class="disabled last" role="tab"><a id="steps-uid-0-t-3" href="#steps-uid-0-h-3" aria-controls="steps-uid-0-p-3"><span class="number">4.</span> Finish</a></li></ul></div><div class="content clearfix">
                                        <h3 class="title current" tabindex="-1" id="steps-uid-0-h-0">Account</h3>
                                        <section aria-hidden="false" class="body current" aria-labelledby="steps-uid-0-h-0" role="tabpanel" id="steps-uid-0-p-0">
                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="userName">User name *</label>
                                                <div class="col-lg-10">
                                                    <input class="form-control required" id="userName" name="userName" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="password"> Password *</label>
                                                <div class="col-lg-10">
                                                    <input id="password" name="password" class="required form-control" type="text">

                                                </div>
                                            </div>

                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="confirm">Confirm Password *</label>
                                                <div class="col-lg-10">
                                                    <input id="confirm" name="confirm" class="required form-control" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <label class="col-lg-12 control-label ">(*) Mandatory</label>
                                            </div>
                                        </section>
                                        <h3 class="title" tabindex="-1" id="steps-uid-0-h-1">Profile</h3>
                                        <section aria-hidden="true" style="display: none;" class="body" aria-labelledby="steps-uid-0-h-1" role="tabpanel" id="steps-uid-0-p-1">
                                            <div class="form-group clearfix">

                                                <label class="col-lg-2 control-label" for="name"> First name *</label>
                                                <div class="col-lg-10">
                                                    <input id="name" name="name" class="required form-control" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="surname"> Last name *</label>
                                                <div class="col-lg-10">
                                                    <input id="surname" name="surname" class="required form-control" type="text">

                                                </div>
                                            </div>

                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="email">Email *</label>
                                                <div class="col-lg-10">
                                                    <input id="email" name="email" class="required email form-control" type="text">
                                                </div>
                                            </div>

                                            <div class="form-group clearfix">
                                                <label class="col-lg-2 control-label " for="address">Address *</label>
                                                <div class="col-lg-10">
                                                    <input id="address" name="address" class="form-control" type="text">
                                                </div>
                                            </div>

                                            <div class="form-group clearfix">
                                                <label class="col-lg-12 control-label ">(*) Mandatory</label>
                                            </div>

                                        </section>
                                        <h3 class="title" tabindex="-1" id="steps-uid-0-h-2">Hints</h3>
                                        <section aria-hidden="true" style="display: none;" class="body" aria-labelledby="steps-uid-0-h-2" role="tabpanel" id="steps-uid-0-p-2">
                                            <div class="form-group clearfix">
                                                <div class="col-lg-12">
                                                    <ul>
                                                        <li>Jonathan Smith </li>
                                                        <li>Lab</li>
                                                        <li>jonathan@smith.com</li>
                                                        <li>Your city, Cityname</li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </section>
                                        <h3 class="title" tabindex="-1" id="steps-uid-0-h-3">Finish</h3>
                                        <section aria-hidden="true" style="display: none;" class="body" aria-labelledby="steps-uid-0-h-3" role="tabpanel" id="steps-uid-0-p-3">
                                            <div class="form-group clearfix">
                                                <div class="col-lg-12">
                                                    <label class="cr-styled">
                                                        <input type="checkbox">
                                                        <i class="fa"></i> 
                                                        I agree with the Terms and Conditions.
                                                    </label>
                                                </div>
                                            </div>
                                        </section>
                                    </div><div class="actions clearfix"><ul role="menu" aria-label="Pagination"><li aria-disabled="true" class="disabled"><a href="#previous" role="menuitem">Previous</a></li><li aria-disabled="false" aria-hidden="false"><a href="#next" role="menuitem">Next</a></li><li aria-hidden="true" style="display: none;"><a href="#finish" role="menuitem">Finish</a></li></ul></div></div>
                                </form> 
                            </div>  <!-- End panel-body -->
                        </div> <!-- End panel -->

