    if(window.XMLHttpRequest)
                {
                        mlhttp=new XMLHttpRequest();
                }
                else
                {
                         mlhttp=new ActiveXObject("Microsoft.XMLHTTP");
                }
                mlhttp.onreadystatechange=function()
                {
        if (mlhttp.readyState===4 && mlhttp.status===200){
                    document.getElementById('viewresults').innerHTML = mlhttp.responseText;              
            }
        };
                mlhttp.open("GET","AjaxSnippets/Details.php?category="+ category + "&id="+idd,true);
                mlhttp.send();  
        }  
