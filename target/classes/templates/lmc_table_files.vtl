                          function show_details(id,category){
        document.getElementById('viewresults').innerHTML = "<img src=\"img/loader.gif\" width='20' height='20' />&nbsp;&nbsp;Please Wait";
        var idd = parseInt(id);
    if(window.XMLHttpRequest)
                {
                        mlhttp=new XMLHttpRequest();
                }
                else
                {
                         mlhttp=new ActiveXObject("Microsoft.XMLHTTP");
                }
                mlhttp.onreadystatechange=function()
                {
        if (mlhttp.readyState===4 && mlhttp.status===200){
                    document.getElementById('viewresults').innerHTML = mlhttp.responseText;              
            }
        };
                mlhttp.open("GET","AjaxSnippets/Details.php?category="+ category + "&id="+idd,true);
                mlhttp.send();  
        }  
                    function clearpasswordnotify(){
        document.getElementById('notify').innerHTML = "<h4>Please Wait...Saving</h4>";
                if(window.XMLHttpRequest)
                {
                        mlhttp=new XMLHttpRequest();
                }
                else
                {
                         mlhttp=new ActiveXObject("Microsoft.XMLHTTP");
                }
                mlhttp.onreadystatechange=function()
                {
        if (mlhttp.readyState===4 && mlhttp.status===200){
                    document.getElementById('notify').innerHTML = "";
                var myScripts = editdiv.getElementsByTagName("script");
            if (myScripts.length > 0) {
                            eval(myScripts[0].innerHTML);
            }
            }
        };
                mlhttp.open("GET","clearnotify.php",true);
                mlhttp.send();  
        }  
